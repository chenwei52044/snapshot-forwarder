{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/snapshot", "dest": "api/snapshot.js" },
    { "src": "/api/forward-to-gpt", "dest": "api/forward-to-gpt.js" },
    { "src": "/api/auto-analyze", "dest": "api/auto-analyze.js" }
  ]
}
